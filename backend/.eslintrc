{
  "extends": "airbnb-base",
  "rules": {
    "no-underscore-dangle": "off",
    "no-param-reassign": "off",
    "no-console": "off",
    "no-unused-vars": "off",
    "quotes": "off",
    "no-unused-expressions": "off",
    "no-shadow": "off",
    "arrow-body-style": "off",
    "import/no-extraneous-dependencies": "off",
    "arrow-parens": "off",
    "no-multi-assign": "off",
    "no-undef": "off",
    "no-else-return": "off",
    "consistent-return": "off",
    "prefer-arrow-callback": "off",
    "func-names": "off",
    "no-unreachable": "off",
    "no-useless-escape": "off"
  }
}